<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="RenStaff" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/mainColor</item>
        <item name="colorPrimaryVariant">@color/mainColor</item>
        <item name="colorOnPrimary">@color/ruby</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/ruby</item>
        <item name="colorSecondaryVariant">@color/ruby</item>
        <item name="colorOnSecondary">@color/ruby</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/RenGreenButton</item>
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:navigationBarColor">@color/black</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>